#!/usr/bin/python

import sys
import re
import types

def usage():
    print "Usage: %s filename clubvaluemax clubvaluemin" % sys.argv[0]
    print "          filename: league table you want to process"
    print "          clubvalues: max/min values of the top/bottom clubs"
    print
    print "Output will be to standard output."

def main():
    try:
        filename = sys.argv[1]
        clubpoints = sys.argv[2]
    except:
        usage()
        sys.exit(1)

    teampattern = re.compile(r' *(\d+)')
    try:
        f = open(filename, 'r')
        fcontents = f.read()
        teamname = teampattern.match(fcontents)
        if type(teamname) != types.NoneType:
            print teamname.groups()[0]
        f.close()
    except IOError:
        print "file %s not found" % filename
        sys.exit(1)
        

if __name__ == '__main__':
    main()
